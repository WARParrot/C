#include <stdio.h>

int main() {
    const int n = 5;
    int A[n], B[n];
    int x = 0;
    int y = 1;
    int f = 1;
    const int rule = 30;
    int num;
    int* a = A;
    int* b = B;
    int* c;
    
    while (x < n) {
        if (x == n / 2) {
            A[x] = 1;
        }
        else {
            A[x] = 0;
        }
        printf("%d", A[x]);
        x += 1;
    }
    x = 0;
    
    while (x < n) {
        B[x] = 0;
        x += 1;
    }
    
    while (f < n) {
        printf("\n");
        y = 0;
        int* x = &a[y];
        while (x == &a[n]) {
            int* x = &a[y];
            if ((*x == 1) || ((*x + 1) == 1) || ((*x - 1) == 1)) {
                num = (a[*x-1]<<2) + (a[*x]<<1) + a[*x+1];
                b[*x] = (rule >> num) & 1;
            }
            y += 1;
        }
        y = 0;
        
        while (y < n) {
           printf("%d", b[y]);
           y += 1;
        }
        
        *a = *c;
        *a = *b;
        *b = *c;
        f += 1;
    }
}
