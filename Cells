#include <stdio.h>

int main() {
    const int n = 31;
    int A[n], B[n];
    int x = 0;
    int f = 1;
    const int rule = 30;
    int num;
    int* a = A;
    int* b = B;
    int* c;
    
    while (x < n) {
        if (x == n / 2) {
            A[x] = 1;
        }
        else {
            A[x] = 0;
        }
        printf("%d", A[x]);
        x += 1;
    }
    x = 0;
    
    while (x < n) {
        B[x] = 0;
        x += 1;
    }
    
    while (f < n) {
        printf("\n");
        x = 0;
        while (&a[x] < &a[n]) {
            if (a[x-1] == 1 || a[x] == 1 || a[x+1] == 1) {
                num = (a[x-1]<<2) + (a[x]<<1) + a[x+1];
                b[x] = (rule >> num) & 1;
            } 
            else if (a[x-1] != 0 && a[x-1] != 1 && (a[x] == 1 || a[x+1] == 1)) {
                num = (0<<2) + (a[x]<<1) + a[x+1];
                b[x] = (rule >> num) & 1;
            }
            x += 1;
        }
        x = 0;
        
        while (x < n) {
           printf("%d", b[x]);
           x += 1;
        }
        
        c = a;
        a = b;
        b = c;
        f += 1;
    }
}
